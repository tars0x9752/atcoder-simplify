{
  "name": "atcoder-simplify",
  "displayName": "atcoder-simplify",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "atcoderSimplify.start",
        "title": "Start AtCoder Simplify",
        "category": "AtCoder Simplify"
      },
      {
        "command": "atcoderSimplify.close",
        "title": "Close AtCoder Simplify",
        "category": "AtCoder Simplify"
      },
      {
        "command": "atcoderSimplify.compile",
        "title": "Compile",
        "category": "AtCoder Simplify"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "fastify": "^2.14.0",
    "fastify-cors": "^3.0.3"
  }
}
